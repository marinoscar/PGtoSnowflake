{
  "name": "pgtosnowflake",
  "version": "0.1.0",
  "description": "CLI tool for migrating PostgreSQL databases to Snowflake",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "pgtosnowflake": "dist/index.js",
    "pg2sf": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
